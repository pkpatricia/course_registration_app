<div class="row space">
  <div class="col-md-3 col-sm-12"><%= image_tag 'college.png', class: 'h1-icon' %></div>
  <div class="col-md-6 course-heading">
    <h1 class="text-center new-font"><strong>Welcome to TTS University</strong></h1>
    
  </div>
</div>

<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    <li data-target="#carousel-example-generic" data-slide-to="3"></li>
    <li data-target="#carousel-example-generic" data-slide-to="4"></li>
  </ol>
  
  

  <!-- Wrapper for slides -->
  <div class="carousel-inner center-block" role="listbox">
      
    <div class="item active">
      <img src="/assets/college.jpg"  alt="..." class="img-responsive center-block">
      <div class="carousel-caption">
        The University
      </div>
    </div>
    <div class="item">
      <img src="/assets/girls_learning.jpg" alt="..." class="img-responsive center-block">
      <div class="carousel-caption">
        The Students
      </div>
    </div>
    <div class="item">
      <img src="/assets/students_in_sun.jpg" alt="..." class="img-responsive center-block">
      <div class="carousel-caption">
        The Rewards
      </div>
    </div>
    <div class="item">
      <img src="/assets/girl_blackboard.jpg" alt="..." class="img-responsive center-block">
      <div class="carousel-caption">
        The Commitment
      </div>
    </div>
    <div class="item">
      <img src="/assets/students.jpg" alt="..." class="img-responsive center-block">
      <div class="carousel-caption">
        The Possibilities
      </div>
    </div>
    
    <%= render partial: "courses/course_listing", object: Course.all %>
    
     </div>
  </div>

